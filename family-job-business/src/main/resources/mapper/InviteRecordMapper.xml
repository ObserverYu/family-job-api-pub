<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.chen.dao.InviteRecordMapper">
 
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.chen.domain.entity.InviteRecord">
        <id column="id" property="id"/>
        <result column="inviter_id" property="inviterId"/>
        <result column="user_id" property="userId"/>
        <result column="inviter_avatar" property="inviterAvatar"/>
        <result column="user_avatar" property="userAvatar"/>
        <result column="state" property="state"/>
        <result column="invite_code" property="inviteCode"/>
        <result column="family_id" property="familyId"/>
        <result column="check_time" property="checkTime"/>
        <result column="create_time" property="createTime"/>
        <result column="quit_time" property="quitTime"/>
        <result column="user_name" property="userName"/>
        <result column="inviter_name" property="inviterName"/>
    </resultMap>
 
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            t.id,
            t.inviter_id,
            t.user_id,
            t.inviter_avatar,
            t.user_avatar,
            t.state,
            t.invite_code,
            t.family_id,
            t.check_time,
            t.create_time,
            t.quit_time,
            t.user_name,
            t.inviter_name,
    </sql>
</mapper>
